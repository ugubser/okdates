{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "dist/okdates/browser",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "trailingSlash": false
  },
  "emulators": {
    "firestore": {
      "port": 8081
    },
    "hosting": {
      "port": 5003
    },
    "ui": {
      "enabled": true,
      "port": 4002
    },
    "hub": {
      "port": 4402
    },
    "logging": {
      "port": 4502
    },
    "singleProjectMode": true,
    "storage": {
      "port": 9199
    }
  },
  "storage": {
    "rules": "storage.rules"
  }
}
